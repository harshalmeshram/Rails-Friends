<div id="<%= dom_id friend %>">
  
  <p><strong>Name : </strong> 
    <%= friend.full_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>
    <strong>Date of Birth:</strong>
      <%= friend.date_of_birth %>
  </p>
   
  <p>
    <strong>Age:</strong>
  
    
    <%= friend.age %>
  


  </p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= friend.twitter %>
  </p>
  <p>
    <strong>User Id:</strong>
    <%#= friend.user id %>
  </p>


</div>
